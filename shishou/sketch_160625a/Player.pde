public class Player{
  
  private int hp = 0;
  private int exp = 0;
  
  public void setHP(int hp){
    this.hp = hp;
  }
  
  public int getHP(){
    return this.hp;
  }
  
  public void setExp(int exp){
    this.exp = exp;
  }
  
  public int getExp(){
    return this.exp;
  }

}